<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="lerga1631730.soundmemory.MainActivity">

    <RelativeLayout
        android:layout_width="376dp"
        android:layout_height="500dp"
        android:layout_marginBottom="80dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="149dp">

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="10dp"
                app:srcCompat="@drawable/l1"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="16dp" />

            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="500px"
                android:layout_height="250px"
                android:layout_alignLeft="@+id/imageView16"
                android:layout_alignParentBottom="true"
                android:layout_alignStart="@+id/imageView16"
                android:layout_marginBottom="35dp"
                app:srcCompat="@drawable/quit"
                android:onClick="Quit"/>

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="500px"
                android:layout_height="250px"
                android:layout_above="@+id/imageView15"
                android:layout_centerHorizontal="true"
                app:srcCompat="@drawable/start"
                android:onClick="Click"/>

        </RelativeLayout>
    </RelativeLayout>

</android.support.constraint.ConstraintLayout>

<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="lerga1631730.soundmemory.GameActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="0dp">

        <Button
            android:id="@+id/btnPause"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="23dp"
            android:layout_marginLeft="29dp"
            android:layout_marginStart="29dp"
            android:onClick="Pause"
            android:text="Pause" />

        <RelativeLayout
            android:id="@+id/Relative"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="150px"
                android:layout_height="150px"
                android:layout_alignTop="@+id/imageView2"
                android:layout_marginEnd="33dp"
                android:layout_marginRight="33dp"
                android:layout_toLeftOf="@+id/imageView2"
                android:layout_toStartOf="@+id/imageView2"
                android:onClick="Musique"
                android:tag="0"
                app:srcCompat="@android:color/holo_blue_dark" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="150px"
                android:layout_height="150px"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="59dp"
                android:onClick="Musique"
                android:tag="1"
                app:srcCompat="@color/colorAccent" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="150px"
                android:layout_height="150px"
                android:layout_alignTop="@+id/imageView2"
                android:layout_toEndOf="@+id/imageView2"
                android:layout_toRightOf="@+id/imageView2"
                android:onClick="Musique"
                android:tag="2"
                android:layout_marginLeft="60px"
                app:srcCompat="@color/colorAccent" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="150px"
                android:layout_height="150px"
                android:layout_alignLeft="@+id/imageView"
                android:layout_alignStart="@+id/imageView"
                android:layout_below="@+id/imageView"
                android:layout_marginTop="36dp"
                android:onClick="Musique"
                android:tag="2"
                app:srcCompat="@android:color/black"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="150px"
                android:layout_height="150px"
                android:layout_alignLeft="@+id/imageView2"
                android:layout_alignStart="@+id/imageView2"
                android:layout_alignTop="@+id/imageView4"
                android:onClick="Musique"
                android:tag="2"
                app:srcCompat="@android:color/holo_purple"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="150px"
                android:layout_height="150px"
                android:layout_alignLeft="@+id/imageView3"
                android:layout_alignStart="@+id/imageView3"
                android:layout_alignTop="@+id/imageView5"
                android:onClick="Musique"
                android:tag="2"
                app:srcCompat="?attr/colorError"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="150px"
                android:layout_height="150px"
                android:layout_alignLeft="@+id/imageView4"
                android:layout_alignStart="@+id/imageView4"
                android:layout_below="@+id/imageView4"
                android:layout_marginTop="24dp"
                android:onClick="Musique"
                android:tag="2"
                app:srcCompat="@color/colorPrimary"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="150px"
                android:layout_height="150px"
                android:layout_alignLeft="@+id/imageView5"
                android:layout_alignStart="@+id/imageView5"
                android:layout_alignTop="@+id/imageView7"
                android:onClick="Musique"
                android:tag="2"
                app:srcCompat="?attr/colorAccent"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="150px"
                android:layout_height="150px"
                android:layout_alignLeft="@+id/imageView6"
                android:layout_alignStart="@+id/imageView6"
                android:layout_alignTop="@+id/imageView8"
                app:srcCompat="@android:color/holo_blue_bright"
                android:onClick="Musique"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="150px"
                android:layout_height="150px"
                android:layout_alignLeft="@+id/imageView7"
                android:layout_alignStart="@+id/imageView7"
                android:layout_below="@+id/imageView7"
                android:layout_marginTop="26dp"
                android:onClick="Musique"
                android:tag="2"
                app:srcCompat="@color/colorPrimaryDark"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="150px"
                android:layout_height="150px"
                android:layout_alignLeft="@+id/imageView8"
                android:layout_alignStart="@+id/imageView8"
                android:layout_alignTop="@+id/imageView10"
                android:onClick="Musique"
                android:tag="2"
                app:srcCompat="@android:color/holo_red_dark"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="150px"
                android:layout_height="150px"
                android:layout_alignLeft="@+id/imageView9"
                android:layout_alignStart="@+id/imageView9"
                android:layout_alignTop="@+id/imageView11"
                android:onClick="Musique"
                android:tag="2"
                app:srcCompat="@android:color/holo_orange_dark"
                tools:ignore="ContentDescription" />

        </RelativeLayout>


    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
